{
  "conversation": {
    "storage_dir": "conversations",
    "max_sessions": 100,
    "session_timeout_hours": 24,
    "context_max_tokens": 100000
  },
  "orchestrator": {
    "max_parallel_tasks": 5,
    "retry_delay_seconds": 1.0,
    "retry_backoff_multiplier": 2.0,
    "max_retries": 3
  },
  "error_recovery": {
    "allow_fallback": true,
    "allow_partial_success": true,
    "max_retries": 3,
    "fallback_map": {
      "browser": ["shell", "app"],
      "chrome_search": ["edge_search", "shell"],
      "type": ["clipboard", "shell"],
      "click": ["shell", "shortcut"]
    }
  },
  "performance": {
    "cache_ttl_seconds": 300,
    "max_cache_size": 1000,
    "rate_limit_per_second": 10.0,
    "max_concurrent_executions": 5,
    "queue_size": 100,
    "cacheable_tools": ["get_weather", "search", "system_info", "scrape"]
  },
  "logging": {
    "level": "INFO",
    "log_file": "logs/agent_enhanced.log",
    "max_bytes": 10485760,
    "backup_count": 5
  }
}
